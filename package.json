{
  "name": "productoapi",
  "version": "1.0.0",
  "description": "api productos",
  "main": "main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "nodemon server.js",
    "start:fork": "nodemon server.js fork 8080",
    "start:cluster": "nodemon server.js cluster 8080",
    "prod:forever": "forever start -w index.js",
    "prod:pm2:fork": "pm2 start index.js --name='ServerFork' --watch",
    "prod:pm2:cluster": "pm2 start index.js --name='ServerCluster' --watch -i max",
    "start:prof": "node --prof index.js FORK 8080",
    "test:artillery": "artillery run artillery.yml",
    "test:autocannon": "autocannon -d 20 -c 100 \"http://localhost:8080/info\"",
    "start:inspect": "node --inspect index.js"
  },
  "author": "fede",
  "license": "ISC",
